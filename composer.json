  },
  "autoload-dev": {
    "psr-4": {
      "AHAbid\\EloquentCassandra\\Tests\\": "tests/",
      "AHAbid\\EloquentCassandra\\Fixtures\\": "tests/fixtures/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
